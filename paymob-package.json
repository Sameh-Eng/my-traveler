{
  "name": "paymob-payment-backend",
  "version": "1.0.0",
  "description": "Complete Paymob payment integration backend for airline booking system",
  "main": "paymob-server.js",
  "scripts": {
    "start": "node paymob-server.js",
    "dev": "nodemon paymob-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "validate": "npm run lint && npm run test",
    "logs": "tail -f logs/combined.log",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "pm2 restart all",
    "pm2:logs": "pm2 logs"
  },
  "keywords": [
    "paymob",
    "payment",
    "integration",
    "egypt",
    "flight-booking",
    "nodejs",
    "express",
    "api"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com",
    "url": "https://yourwebsite.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "crypto": "^1.0.1",
    "mysql2": "^3.6.3",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "@types/node": "^20.9.0",
    "pm2": "^5.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**",
      "!paymob-package.json"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "node": true,
      "es2021": true,
      "jest": true
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "warn",
      "prefer-const": "error"
    }
  },
  "nodemonConfig": {
    "watch": ["paymob-server.js", "backend_paymob_integration.js", "paymob-api-routes.js"],
    "ext": "js,json",
    "ignore": ["node_modules/", "coverage/", "logs/"],
    "env": {
      "NODE_ENV": "development"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/paymob-payment-backend.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/paymob-payment-backend/issues"
  },
  "homepage": "https://github.com/yourusername/paymob-payment-backend#readme",
  "files": [
    "paymob-server.js",
    "backend_paymob_integration.js",
    "paymob-api-routes.js",
    ".env.paymob.example",
    "README.md"
  ],
  "directories": {
    "lib": "lib",
    "test": "__tests__",
    "docs": "docs"
  }
}