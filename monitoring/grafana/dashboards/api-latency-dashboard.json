        {
          "expr": "sum(rate(http_requests_total{service=\"mytraveler-backend\",status=~\"4..\"}[5m])) / sum(rate(http_requests_total{service=\"mytraveler-backend\"}[5m])) * 100",
          "interval": "",
          "legendFormat": "4xx Errors",
          "refId": "B"
